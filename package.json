  "description": "A design system boilderplate",
  "directories": {
    "lib": "lib"
  },
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "build-storybook",
    "linter": "eslint --quiet --cache 'lib/**/*.ts' 'lib/**/*.tsx'",
    "linter:fix": "npm run linter -- --fix",
    "start": "start-storybook -p 6006 -s ./public --ci",
